<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
  <meta charset="UTF-8">
  <title data-base="{{ title|default('MakeanIco') }} ">{{ title|default('MakeanIco') }}</title>

  <meta name="viewport" content="width=device-width, initial-scale=1">

  {% if loadStyles|default(true) %}
  <link rel="stylesheet" href="assets/css/main{% if production %}.min{% endif %}.css">
  {% endif %}

  {% if saveData|default(false) %}
    <link id="favicon" rel="icon" type="image/png" href="/favicon.png" />
    <meta property="og:image" content="{{ baseUrl|default('https://makeanico.azurewebsites.net/') }}og.png">
  {% else %}
    <link id="favicon" rel="icon" type="image/png" href="{% if startOver %}/make/favicon.png?{{ cellURL }}{% else %}/favicon.png{% endif %}" />
    <meta property="og:image" content="{{ baseUrl|default('https://makeanico.azurewebsites.net/')  }}{% if startOver %}make/256/favicon.png?{{ cellURL }}{% else %}og.png{% endif %}">

    <meta property="og:description" content="Makeanico is a progressively enhanced web app that allows anyone to create a 16x16 favicon.ico graphic.">
    <meta property="og:image:width" content="256">
    <meta property="og:image:height" content="256">

    {% if fill %}
      <meta name="theme-color" content="{{ fill|replace({'0x':'#'}) }}">
      <meta name="msapplication-navbutton-color" content="{{ fill|replace({'0x':'#'}) }}">
      <meta name="apple-mobile-web-app-capable" content="yes">
      <meta name="apple-mobile-web-app-status-bar-style" content="{{ fill|replace({'0x':'#'}) }}">
    {% endif %}
  {% endif %}
